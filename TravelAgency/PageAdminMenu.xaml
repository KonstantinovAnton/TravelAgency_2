<Page x:Class="TravelAgency.PageAdminMenu"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:TravelAgency"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="PageAdminMenu">

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition Height="5*"></RowDefinition>
        </Grid.RowDefinitions>
        
        <TextBlock FontSize="50">Меню админа</TextBlock>
        <StackPanel Grid.Row="1">
            <Button Name ="gotoPageAdminShowData" Grid.Row="1" Width="350" Height="50" Click="gotoPageAdminShowData_Click">Пользователи</Button>
            <Button Name ="gotoPageAdminTour"  Width="350" Height="50" Click="gotoPageAdminTour_Click">Основаня таблица</Button>
            </StackPanel>
    </Grid>
</Page>
